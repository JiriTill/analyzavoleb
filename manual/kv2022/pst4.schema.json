{
	"$id": "https://www.volby.cz/ps/PS_T4.schema.json",
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "PS - výsledky za volební okrsky",
	"description": "Data z tiskopisu T/4 zápisu komise v kompletní podobě (široká struktura)",
	"type": "object",
	"properties": {
		"typ": {
			"type": "string"
		},
		"iri": {
			"type": "string",
			"format": "iri",
			"title": "Identifikátor"
		},
		"název": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/2020-07-01/schémata/text.json"
		},
		"akronym": {
			"type": "string"
		},
		"platnost": {
			"$ref": "https://ofn.gov.cz/základní-datové-typy/2020-07-01/schémata/časový-interval.json"
		},
		"polozky": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"typ": {
						"const": "Položka"
					},
					"iri": {
						"type": "string",
						"format": "iri",
						"title": "Identifikátor"
					},
					"ID_OKRSKY": {
						"type": "integer",
						"description": "Odkaz na identifikaci okrsku (FK)",
						"maximum": 999999
					},
					"TYP_FORM": {
						"type": "integer",
						"description": "Obsah dat ve větě (1 = T/4, 2 = příloha T/4, 3 = součtová věta TS)",
						"maximum": 9
					},
					"OPRAVA": {
						"type": "integer",
						"description": "Příznak dodatečné opravy na přebíracím místě v případě chyby (1 = opraven, 0 = neopraven)",
						"maximum": 9
					},
					"CHYBA": {
						"type": "integer",
						"description": "Příznak chyby (pustitelné) v datech (1 = je chyba, 0 = není chyba)",
						"maximum": 9
					},
					"OKRES": {
						"type": "integer",
						"description": "Kód okresu dle CNUMNUTS",
						"maximum": 9999
					},
					"OBEC": {
						"type": "integer",
						"description": "Kód obce dle COCO",
						"maximum": 999999
					},
					"OKRSEK": {
						"type": "integer",
						"description": "Číslo okrsku v rámci obce",
						"maximum": 99999
					},
					"KC_1": {
						"type": "integer",
						"description": "Kontrolní číslo 1 (MODULO7)",
						"maximum": 9
					},
					"KC_2": {
						"type": "integer",
						"description": "Kontrolní číslo 2",
						"maximum": 999999
					},
					"ZAKRSTRANA": {
						"type": "string",
						"description": "Příznak zakroužkované strany (pořadí znaku odpovídá vylosovanému číslu strany) (1 = zakroužkována, 0 = nezakroužkována)",
						"maxlength": 60
					},
					"VOL_SEZNAM": {
						"type": "integer",
						"description": "Počet osob, zapsaných do výpisu ze stálého a zvláštního seznamu",
						"maximum": 99999
					},
					"VYD_OBALKY": {
						"type": "integer",
						"description": "Počet voličů, kterým byly vydány úřední obálky",
						"maximum": 99999
					},
					"DOR_OBALKY": {
						"type": "integer",
						"description": "Počet otevřených doručovacích obálek pro korespondenční volbu",
						"maximum": 99999
					},
					"DOR_ODLOZ": {
						"type": "integer",
						"description": "Počet odložených doručovacích obálek",
						"maximum": 99999
					},
					"ODEVZ_OBAL": {
						"type": "integer",
						"description": "Počet odevzdaných úředních obálek",
						"maximum": 99999
					},
					"PL_HL_CELK": {
						"type": "integer",
						"description": "Počet platných hlasů ve volebním okrsku celkem",
						"maximum": 99999
					}
				},
				"additionalProperties": false,
				"required": ["ID_OKRSKY"]
			}
		}
	},
	"required": [
		"iri",
		"typ",
		"název",
		"polozky"
	]
}
